package metodos;

public class Numeros {

	public static void sumarNumero(Integer num) {
		int numero = 1;
		for (int i = 1; i <= num; i++) {
			System.out.println(numero = numero + numero);
		}	
	}
	public static void factorial(Integer num) {
		int numero = 1;
		int total = 0;
		int mult = 0;
		for (int i = 1; i <= num; i++) {
			System.out.println(total = numero * mult);
			mult += 1;;
	}
}
	}
